config system dns
	set primary {{shglobal_dns1}}
	set secondary {{shglobal_dns2}}
	set protocol {{shglobal_dnsprotocol}}
end
config system ntp
	set server-mode disable
    set ntpsync enable
    set type custom
    config ntpserver
    	edit 1
			set server {{shglobal_ntp}}
        next
    end
end
